{
  "name": "ember-advanced-combobox",
  "version": "3.0.4",
  "description": "Swiss-knife ember combobox",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "homepage": "https://github.com/AnnotationSro/ember-advanced-combobox",
  "engines": {
    "node": "10.* || >= 12"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "~3.21.2",
    "ember-cli-app-version": "3.2.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-update": "0.54.6",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "8.0.2",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-modal-dialog": "3.0.0-beta.3",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.11.0",
    "ember-truth-helpers": "2.1.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.8.0",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-node": "^11.1.0",
    "express": "^4.8.5",
    "glob": "^7.1.3",
    "loader.js": "4.7.0",
    "morgan": "^1.3.2",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.4.0"
  },
  "keywords": [
    "ember-addon",
    "combobox",
    "dropdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AnnotationSro/ember-advanced-combobox"
  },
  "author": "Annotation",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "cash-dom": "8.1.0",
    "ember-auto-import": "1.6.0",
    "element-resize-detector": "1.2.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-node-assets": "^0.2.2",
    "popper.js": "1.14.6"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">= 2.0.0"
    }
  },
  "volta": {
    "node": "14.13.0"
  }
}
