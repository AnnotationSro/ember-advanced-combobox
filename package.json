{
  "name": "ember-advanced-combobox",
  "version": "4.0.0",
  "description": "Swiss-knife ember combobox",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "homepage": "https://github.com/AnnotationSro/ember-advanced-combobox",
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^0.43.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^8.0.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "~3.28.0",
    "ember-cli-app-version": "4.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-test-loader": "3.0.0",
    "ember-cli-update": "1.0.0",
    "ember-decorators": "6.1.1",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "8.0.4",
    "ember-load-initializers": "^2.1.2",
    "ember-modal-dialog": "3.0.3",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-truth-helpers": "3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "loader.js": "4.7.0",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0"
  },
  "keywords": [
    "ember-addon",
    "combobox",
    "dropdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AnnotationSro/ember-advanced-combobox"
  },
  "author": "Annotation",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "cash-dom": "8.1.0",
    "element-resize-detector": "1.2.3",
    "ember-auto-import": "1.11.3",
    "ember-classic-decorator": "^2.0.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-node-assets": "^0.2.2",
    "popper.js": "1.14.6"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">= 2.0.0"
    }
  },
  "volta": {
    "node": "14.13.0"
  }
}
